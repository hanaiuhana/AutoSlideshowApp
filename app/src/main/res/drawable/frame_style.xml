<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:color="@color/color_navy_main" >
        <shape
            android:shape="rectangle" >
            <!-- 枠線 width:線の幅、color:線の色 -->
            <stroke
                android:width="1.5dp"
                android:color="@color/color_navy_main"
                />
        </shape>
    </item>
    <item android:state_enabled="false" android:color="@color/color_gray_off" >
        <shape
            android:shape="rectangle" >
            <!-- 枠線 width:線の幅、color:線の色 -->
            <stroke
                android:width="1.5dp"
                android:color="@color/color_gray_off"
                />
        </shape>
    </item>
